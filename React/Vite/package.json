{
  "name": "server_side_rendering_demos",
  "private": true,
  "version": "1.0.0",
  "author": "David Bishop",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dBish6/Server_Side_Rendering_Demos"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.5.0"
  },
  "scripts": {
    "dev": "dotenv -e .env.development tsx server.ts",
    "build": "npm run build:client && npm run build:ssr",
    "build:client": "vite build",
    "build:ssr": "rm -rf build/server.mjs build/assets && vite build --ssr server.ts",
    "start": "dotenv -e .env.production node build/server.mjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "dotenv-cli": "^7.4.2",
    "express": "4.21.2",
    "express-rate-limit": "^7.3.0",
    "morgan": "^1.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.29.0",
    "sirv": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.23",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.8.3",
    "tsx": "^4.20.0",
    "vite": "6.3.5"
  }
}
